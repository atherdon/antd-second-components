    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.1",
    "shortid": "^2.2.14",
    "uuid": "^3.3.2"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
